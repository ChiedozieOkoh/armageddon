define register_trace
   set $asm_fn_line = LINE
   echo <<STARTING_PROC_LOG>>\n
   display $r0
   display $r1
   display $r2
   display $r3
   display $sp
   display $pc
   display $lr
   display $xPSR
   continue
   echo <<-->>\n
   while($asm_fn_line > 0)
      stepi
      echo <<-->>\n
      set $asm_fn_line = $asm_fn_line - 1
   end
end

target remote :3333
monitor reset init
break test_start 
load
b BREAKPOINT
continue
register_trace
echo <<FINISHED_PROC_LOG>>\n
